<?xml version="1.0" ?>
<robot name="2WD_rover" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find rr_openrover_description)/urdf/common/common_properties.xacro"/>
  <xacro:include filename="$(find rr_openrover_description)/urdf/2WD_Rover/2WD_rover.gazebo.xacro"/>
  <xacro:include filename="$(find rr_openrover_description)/urdf/accessories/accessories.urdf.xacro" />

  <link name="base_footprint"/>

  <link name="base_link"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="chassis_link">
    <visual>
      <origin xyz="0 0 0" rpy="${pi / 2} 0 ${pi / 2}"/>
      <geometry>
        <mesh filename="package://rr_openrover_description/meshes/2WD_Rover/2WDCore.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>
    <collision>
      <origin xyz="-0.142 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.42 0.25 0.12"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.142 0 0" rpy="0 0 0" />
      <mass value="7.5" />
      <inertia ixx="1.340e-1"
               ixy="0.0" iyy="1.713e-1"
               ixz="0.0" iyz="1.815e-3" izz="5.917e-2" />
    </inertial>
  </link>

  <joint name="chassis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis_link"/>
    <origin xyz="0 0 0.14" rpy="0 0 0"/>
  </joint>

  <link name="left_front_wheel_link">
    <visual>
      <origin xyz="0 -0.18 0" rpy="0 0 ${pi / 2}"/>
      <geometry>
        <mesh filename="package://rr_openrover_description/meshes/2WD_Rover/LeftFrontTire.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${pi / 2} 0 0"/>
      <geometry>
        <cylinder length="0.07" radius="0.127"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.25" />
      <inertia ixx="7.750e-3"
               ixy="0.0" iyy="4.546e-3"
               ixz="0.0" iyz="1.478e-5" izz="4.545e-3" />
    </inertial>
  </link>

  <joint name="left_front_wheel_joint" type="continuous">
    <parent link="chassis_link"/>
    <child link="left_front_wheel_link"/>
    <origin xyz="0 0.18 0" rpy="0 0 0"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <link name="right_front_wheel_link">
    <visual>
      <origin xyz="0 0.18 0" rpy="0 0 ${pi / 2}"/>
      <geometry>
        <mesh filename="package://rr_openrover_description/meshes/2WD_Rover/RightFrontTire.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${pi / 2} 0 0"/>
      <geometry>
        <cylinder length="0.07" radius="0.127"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.25" />
      <inertia ixx="7.750e-3"
               ixy="0.0" iyy="4.546e-3"
               ixz="0.0" iyz="1.478e-5" izz="4.545e-3" />
    </inertial>
  </link>

  <joint name="right_front_wheel_joint" type="continuous">
    <parent link="chassis_link"/>
    <child link="right_front_wheel_link"/>
    <origin xyz="0 -0.18 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="left_rear_castor_housing_link">
    <visual>
      <origin xyz="0.355 -0.171 0" rpy="${pi / 2} 0 ${pi / 2}"/>
      <geometry>
        <mesh filename="package://rr_openrover_description/meshes/2WD_Rover/LeftRearCastorHousing.stl" scale="0.001 0.001 0.001"/>
      </geometry>ps -aux | grep gazebo | awk $2
      <material name="dark"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.25" />
      <inertia ixx="4.81e-5"
               ixy="0.0" iyy="2.939e-5"
               ixz="0.0" iyz="1.478e-5" izz="4.914e-5" />
    </inertial>
  </link>

  <joint name="left_rear_castor_housing_joint" type="continuous">
    <parent link="chassis_link"/>
    <child link="left_rear_castor_housing_link"/>
    <origin xyz="-0.355 0.175 0.005" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="left_rear_castor_wheel_link">
    <visual>
      <origin xyz="0.385 -0.170 0.083" rpy="${pi / 2} 0 ${pi / 2}"/>
      <geometry>
        <mesh filename="package://rr_openrover_description/meshes/2WD_Rover/LeftRearCastorWheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${pi / 2} 0 0"/>
      <geometry>
        <cylinder length="0.0254" radius="0.050"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.25" />
      <inertia ixx="2.511e-4"
               ixy="0.0" iyy="2.136e-4"
               ixz="0.0" iyz="1.478e-5" izz="1.363e-4" />
    </inertial>
  </link>

  <joint name="left_rear_castor_wheel_joint" type="continuous">
    <parent link="left_rear_castor_housing_link"/>
    <child link="left_rear_castor_wheel_link"/>
    <origin xyz="-0.028 0 -0.08" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="right_rear_castor_housing_link">
    <visual>
      <origin xyz="0.355 0.171 0" rpy="${pi / 2} 0 ${pi / 2}"/>
      <geometry>
        <mesh filename="package://rr_openrover_description/meshes/2WD_Rover/RightRearCastorHousing.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.25" />
      <inertia ixx="4.81e-5"
               ixy="0.0" iyy="2.939e-5"
               ixz="0.0" iyz="1.478e-5" izz="4.914e-5" />
    </inertial>
  </link>

  <joint name="right_rear_castor_housing_joint" type="continuous">
    <parent link="chassis_link"/>
    <child link="right_rear_castor_housing_link"/>
    <origin xyz="-0.355 -0.175 0.005" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <link name="right_rear_castor_wheel_link">
    <visual>
      <origin xyz="0.385 0.171 0.083" rpy="${pi / 2} 0 ${pi / 2}"/>
      <geometry>
        <mesh filename="package://rr_openrover_description/meshes/2WD_Rover/RightRearCastorWheel.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${pi / 2} 0 0"/>
      <geometry>
        <cylinder length="0.0254" radius="0.050"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.25" />
      <inertia ixx="2.511e-4"
               ixy="0.0" iyy="2.136e-4"
               ixz="0.0" iyz="1.478e-5" izz="1.363e-4" />
    </inertial>
  </link>

  <joint name="right_rear_castor_wheel_joint" type="continuous">
    <parent link="right_rear_castor_housing_link"/>
    <child link="right_rear_castor_wheel_link"/>
    <origin xyz="-0.028 0 -0.08" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="mount_point_link"/>

  <joint name="mount_point_joint" type="fixed">
    <parent link="chassis_link"/>
    <child link="mount_point_link"/>
    <origin xyz="-0.142 0 0.06" rpy="0 0 0"/>
  </joint>
</robot>
